{
  "name": "thehuijb.github.io",
  "version": "1.0.0",
  "description": "github pages blog",
  "main": "eleventy.js",
  "scripts": {
    "watch:sass": "sass --watch src/_sass:_site/assets/styles",
    "build:sass": "sass ./src/_sass:./_site/assets/styles",
    "watch:eleventy": "eleventy --serve --watch",
    "build:eleventy": "eleventy",
    "start": "npm-run-all build:sass --parallel watch:*",
    "build": "npm run clean && npm-run-all build:*",
    "clean": "rm -rf ./_site"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thehuijb/thehuijb.github.io.git"
  },
  "keywords": [
    "github",
    "pages",
    "gh-pages",
    "11ty"
  ],
  "author": "thehuijb",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thehuijb/thehuijb.github.io/issues"
  },
  "homepage": "https://github.com/thehuijb/thehuijb.github.io#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "markdown-it": "^12.0.6",
    "npm-run-all": "^4.1.5",
    "sass": "^1.34.1"
  }
}
